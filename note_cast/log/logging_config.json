{
    "logger": {
        "path": "./app/core/log/logs_files",
        "filename": "dev.log",
        "level": "info",
        "rotation": "20 days",
        "retention": "1 months",
        "format": "<u><blue>[LOGURU]</blue></u>  <level>{level: <8}</level> <green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> request id: {extra[request_id]} - <cyan>{name}</cyan>:<cyan>{function}</cyan> - <level>{message}</level>"

    }
}